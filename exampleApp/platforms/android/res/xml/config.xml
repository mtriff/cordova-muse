<?xml version='1.0' encoding='utf-8'?>
<widget id="com.example.muse" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Muse">
        <param name="android-package" value="com.mtriff.cordova.muse.plugin.Plugin" />
    </feature>
    <name>MuseExampleApp</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
</widget>
